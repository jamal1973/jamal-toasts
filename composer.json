{
  "name": "jamal/toasts",
  "description": "Universal, polished toast notifications for Laravel (Blade).",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "illuminate/support": "^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "Jamal\\Toasts\\": "src/"
    },
    "files": [
      "src/Support/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jamal\\Toasts\\ToastsServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}